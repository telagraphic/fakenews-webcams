<main class="page__main" data-barba="container" data-barba-namespace="reader">
  <section class="reader">
    <article class="reader__news-section">
      <section class="reader__news-container">
        <section class=" reader__news-row">
          <article id="cnn" class="reader__news-column newspaper">
            <header class="newspaper__header">
              <h1 class="newspaper__header-title">CNN</h1>
            </header>
            <a href="{{CNN_HEADLINE.href}}" class="newspaper__hero">
              <img class="newspaper__hero-image" src="https://{{CNN_HEADLINE.img}}" alt="">
            </a>
            <section class="newspaper__headline">
              <a href="{{CNN_HEADLINE.href}}" clas="newspaper__headline-link">
                <h2 class="newspaper__headline-link-header">{{CNN_HEADLINE.headline}}</h2>
              </a>
            </section>
            <ul class="newspaper__stories">
              {{#each CNN_STORIES}}
              <li class="newspaper__story">
                <a class="newspaper__story-link" href="{{this.href}}">
                  <h3 class="newspaper__story-link-text">
                    {{this.headline}}
                  </h3>
                </a>
              </li>
              {{/each}}
            </ul>
          </article>
