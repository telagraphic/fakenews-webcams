                  <h3 class="newspaper__story-link-text">
                    {{this.headline}}
                  </h3>
                </a>
              </li>
              {{/each}}
            </ul>
          </article>
        </section>
        <section class="news-page__news-video reader__tv-row tv">
          <section class="news-page__news-video-wrapper tv__wrapper">
            <iframe class="news-page__news-video-iframe tv__iframe" src="https://www.youtube-nocookie.com/embed/Gm3bQVANtVo?controls=0&rel=0&autoplay=1" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" frameborder="0"
              allowfullscreen></iframe>
          </section>
        </section>
